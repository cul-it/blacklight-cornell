
    <header class="head">
 <div>
    <a href="#content" accesskey="S" onclick="$('#content').focus();" class="offscreen">Skip to Main Content</a>
  </div>

  <div class="container">
    <div class="cornell-logo d-none d-sm-block">
      <a href="http://www.cornell.edu" class="insignia">Cornell University</a>
      <div class="library-brand">
        <a href="https://library.cornell.edu">Library</a>
      </div>
    </div>
    <div class="d-block d-sm-none">
      <a class="mobile-cornell-logo" href="https://www.cornell.edu">Cornell University</a>
      <a class="library-brand-mobile" href="https://library.cornell.edu">Library</a>
    </div>
    <h1>Division of Rare and Manuscript Collections</h1>
  </div>
</header>
<%#= params.inspect %>
<% paramsHash = params.to_h %>
 
<div class="container">
    <% requestsString = "" %>
    <% requestsArray = [] %>
    <% paramsHash.each do |key, value| %>
       <% if key == "Request" %>
           <% value.each do | val | %>
               <% requestsString = requestsString + " || " + "Request" + "=" + val %>
           <% end %>
           <%  requestsArray = requestsString.split(" || ") %>
       <% end %>
    <% end %>
    <%= requestsArray.inspect %>
    <% requestsArray.each do | request | %>
       <% if !request.blank? %>
        <% requestOne = request.split('=') %>
        <% paramsHash[requestOne[0]] = requestOne[1] %>
      <% end %>
    <% end %>
    <% if paramsHash[:Request].kind_of?(Array) %>
      <% paramsHash[:Request] = paramsHash[:Request].to_h %>
   <% end %>
   
    <%= paramsHash.inspect %>

     
  <form action="https://rmc-aeon.library.cornell.edu/aeon/boom.html" method="GET">
      <input type="hidden" name="authenticated" value="true" />
      <input type="hidden" name="target" value="<%= ENV['AEON_RETURN_TARGET'] %>/aeon/redirect_shib" />
      <input type="hidden" name="aeonParams" value="<%= paramsHash %>" />
      <input type="submit" value="Cornell Affiliates With a NetID Enter Here" style="font-size: 13px; width: 300px;" />
    </form>

         <form action="/aeon/redirect_nonshib" method="POST" style="margin-top: 30px;">
           <input type="hidden" name='newParams' value='<%= paramsHash %>'>
      
        <input type="submit" value="Public Users Without a Cornell NetID Enter Here" style="font-size: 13px; width: 300px;" />
    </form>
    <%#= @paramsString.inspect %>
</div>
</div>
</div>


				
