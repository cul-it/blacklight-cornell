    <%# paramsHash.inspect %>

     
  <form id='aeonPoster' action='https://rmc-aeon.library.cornell.edu/aeon/aeon.dll' method='POST'>
  <!-- form action="https://rmc-aeon.library.cornell.edu/aeon/boom_login.html" method="GET" --> 
  <!-- form action="/aeon/redirect_shib" method="POST" style="margin-top: 30px;"> -->
     <input type="hidden" name="authenticated" value="true" />
      <input type="hidden" name="target" value="https://rmc-aeon.library.cornell.edu/aeon/aeon.dll" method="POST" style="margin-top: 30px;" >
       <input type="hidden" name="aeonControl_usertype" value="shib" >
      <!-- input type="hidden" name="aeonParams" value="<%= paramsHash %>" / -->
             <% paramsHash.each do |key, value| %>
                  <% if key == "Request" %>
                    <% value.each do |val| %>
                  	       <input type="hidden" name="<%= key %>" value="<%= val %>" >
                    <% end %>
                 <% else %>
                   <input type="hidden" name="<%= key %>" value="<%= value %>">
                 <% end %>
               
            <% end %>
  
      <input type="submit" value="Cornell Affiliates With a NetID Enter Here" style="font-size: 13px; width: 300px;" />
    </form>

         <form action="/aeon/redirect_nonshib" method="POST" style="margin-top: 30px;">
           <input type="hidden" name='newParams' value='<%= paramsHash %>'>
      
        <input type="submit" value="Public Users Without a Cornell NetID Enter Here" style="font-size: 13px; width: 300px;" />
    </form>
    <%#= @paramsString.inspect %>
