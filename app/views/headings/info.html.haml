-if @headingsResponse.present? 
	-@headingsResponse.inspect
	- @headingsResponse.each do |data|
		%h2
			=data["heading"]
		(title count)

		%dl
			- if !data["notes"].nil?
				%dt
					Scope note
				%dd
					-data["notes"].each do |n|
						=n
			%dt
				Reference info
			%dd
				.reference
					-if !data["seeAlso"].nil?
						%em
							See also:
						- data["seeAlso"].each do |a|
							-a_link = a.split("|")[0]
							-a_note = a.split("|")[1]
							.see-ref
								- if a_note.present?
									=a_note + ':'

								- if a_note.nil?
									See also:
								=link_to "/headings/info?q=" + a_link + "&start=0" do
									=a_link
									%span.label.label-info
										Info
							.author-works
								Works by:
								= link_to '/?q="' + a + '"&search_field=author%2Fcreator' do
									(title count)
							.author-works
								Works about:
								= link_to '/?q="' + a + '"&search_field=subject' do
									(title count)
			%dt
				Headings type
			%dd
				=data["headingTypeDesc"]


	
