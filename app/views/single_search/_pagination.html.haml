.row
	.span12
		.pagination.pagination-mini.navbar
			= paginate @results.pagination,{:theme => 'blacklight'}
			.web-count
				- if @results.total_items && @results.total_items > 0
					Showing
					= number_with_delimiter @results.pagination.start_record
					through
					= number_with_delimiter @results.pagination.end_record
					of
					= number_with_delimiter @results.pagination.count_records
					(fetched in
					= "%.3f" % @results.timing
					s)