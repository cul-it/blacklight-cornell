<% if @response.num_pages > 1 %>
 <div class="row record-padding">
  <div class="span9">
    <div id="sticky" class="pagination pagination-mini pagination-centered navbar navbar-fixed-bottom">
      <%= paginate @response, :outer_window => 2, :theme => 'blacklight' %>
    </div>
  </div>
 </div>
<% end %>
